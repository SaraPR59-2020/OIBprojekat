<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
	<system.serviceModel>
		<services>
			<service name="Subscriber.SubForEngine">
				<host>
					<baseAddresses>
						<add baseAddress="net.tcp://localhost:8888" />
					</baseAddresses>
				</host>
				<!-- Service Endpoints -->
				<endpoint address="ISubForEngine" binding="netTcpBinding"
						contract="Common.ISubForEngine" />
			</service>

		</services>
	</system.serviceModel>
</configuration>